#ifdef __USE_CMSIS
#include "LPC17xx.h"
#endif

#include <cr_section_macros.h>
#include "eeprom.h"

#include <stdio.h>

// TODO: insert other include files here

// TODO: insert other definitions and declarations here

int main(void) {

	EEPROM_Init();

    char * test = "Fortnite";

    EEPROM_Write(test, 9);

    char read [9];

    EEPROM_Read(read, 9);

    printf("Read: %s", read);

    char * test2 = "SI";

    EEPROM_Write(test2, 3);

    EEPROM_Read(read, 3);

    printf("Read: %s", read);

    return 0 ;
}
