#include "I2C.h"


void EEPROM_Init(){
	I2C_Init();
}

int EEPROM_Write(char * buffer, int length){
	if(status == I2C_STARTED)
		return 0; //busy
	if(length > I2C_BUFFER_SIZE)
		return -1; // buffer to big

	for(int i = 0; i < lenght; i++)
		Wrbuffer[i] = buffer[i];

	WrLength = length;

	I2C_Start();
}

