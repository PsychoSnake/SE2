#ifdef __USE_CMSIS
#include "LPC17xx.h"
#endif

#include <cr_section_macros.h>
#include "eeprom.h"

#include <stdio.h>

// TODO: insert other include files here

// TODO: insert other definitions and declarations here

int main(void) {

	EEPROM_Init();

    char * test = "Diogo e pato";

    EEPROM_Write(test, 13);

    char read [13];

    int i = 10000000;
    while(i-->0);

    EEPROM_Read(read, 13);

    printf("Read: %s", read);

    char * test2 = "Tiaga a noba";

    i = 10000000;
	while(i-->0);

    EEPROM_Write(test2, 13);

    i = 10000000;
	while(i-->0);

    EEPROM_Read(read, 13);

    printf("Read: %s", read);

    return 0 ;
}
